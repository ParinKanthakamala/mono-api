{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@system/*": [
        "src/system/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
      "@rabbitmq-service/*": [
        "src/modules/rabbitmq/*"
      ],
      "@libraries/*": [
        "src/system/libraries/*",
        "src/libraries/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@exceptions/*": [
        "src/exceptions/*"
      ],
      "@controllers/*": [
        "src/controllers/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ]
    }
  }
}
